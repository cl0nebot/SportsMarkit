	  <%= form_tag import_uploads_path, multipart: true do %>
	  
	  <%= select_tag :object, options_for_select([ "School", "Team", "Club", "League" ], "School"), id: "object-field" %>
      <%= select_tag :school_id, options_for_select(School.all.collect{ |school| [school.name, school.id] }), {:multiple => false, include_blank: true} %><br>
	  <div class="fileinput fileinput-new input-group" data-provides="fileinput">
	    <div class="form-control" data-trigger="fileinput"><i class="glyphicon glyphicon-file fileinput-exists"></i> <span class="fileinput-filename"></span></div>
	    <span class="input-group-addon btn btn-default btn-file"><span class="fileinput-new">Select file</span><span class="fileinput-exists">Change</span><%= file_field_tag :file, id: "file-field" %></span>
	    <a href="#" class="input-group-addon btn btn-default fileinput-exists" data-dismiss="fileinput">Remove</a>
	  </div>
	    <div id="invalid-message" style="display: none">
			<font size="2" color="red">Oops. Invalid file type. Please try again.</font>
		</div>
   
	    <%= submit_tag "Import Schools",class: "btn btn-success btn", disabled: true, id: "file-submit" %>
	  <% end %>
      
	  
	  
