    <%= link_to "View Signed Documents", eval("#{klass}_signed_documents_path(@object)") %></br>
     <% unless ["facility"].include? klass %>
      <%= link_to "Upgrade Account", eval("#{klass}_upgrade_path(@object)") %></br>
    <% end %>
    <% unless ["team", "facility"].include? klass %>
      <%= link_to "Add Facility", eval("new_#{klass}_facility_path(@object)") %></br>
    <% end %>
    <%= link_to "Return to Profile", eval("#{klass}_path(@object)") %></br>
    <%= link_to "View/Manage Registrations", eval("#{klass}_registrations_path(@object)") %></br>
    <%= link_to "Bank Account", eval("#{klass}_bank_account_index_path(@object)") %></br>
