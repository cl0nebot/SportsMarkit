<% klass = @object.class.to_s.downcase %>
<% unless klass == "user"%>
    <% unless ["team", "facility"].include? klass %>
      <%= link_to "Add Team", eval("new_#{klass}_team_path(@object)") %></br>
    <% end %>
    <%= link_to "Add Event", eval("new_#{klass}_event_path(@object)") %></br>
    <%= link_to "Send Announcement", eval("#{klass}_announcements_path(@object)") %></br>
