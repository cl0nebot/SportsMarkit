<% unless @accepting_action %>
  $('#edit-member-<%= @class %><%= @id %>').empty()
  $('#edit-member-<%= @class %><%= @id %>').append('<%= j render 'teams/admin_view_member', m: @object, team: @team %>')
  $('#edit-head-<%= @class %><%= @id %>').empty()
  $('#edit-head-<%= @class %><%= @id %>').append('<%= j render 'teams/head', m: @object, team: @team %>')
<% else %>
  $('#pending-<%= @role.id %>').empty()
  $('#athletes-of-team').empty()
  $('#athletes-of-team').append('<%= j render 'teams/athletes_of_team', members: @members, team: @team %>')
  $('#heads-of-team').empty()
  $('#heads-of-team').append('<%= j render 'teams/heads_of_team', heads: @heads, team: @team %>')
  $('#coach-count').empty()
  $('#coach-count').append("<%= j render 'teams/coach_count', heads: @heads %>")
  $('#athlete-count').empty()
  $('#athlete-count').append("<%= j render 'teams/athlete_count', members: @members %>")
<% end %>
