<% unless AthleticDirector.where(school_id: school.id, user_id: current_user.id).present? %>
	<%= form_for AthleticDirector.new, url: user_add_athletic_director_at_setup_path(current_user), html: {data: {remote: true}} do |f| %>
	  <%= f.hidden_field :school_id, value: school.id %>
	  <%= f.hidden_field :user_id, value: current_user.id %>
	  <%= f.submit "Add School", class: "btn btn-block btn-primary" %>
	<% end %>
<% else %>
	<%= form_for AthleticDirector.new, url: user_add_athletic_director_at_setup_path(current_user), html: {data: {remote: true}} do |f| %>
	  <%= f.submit "Claimed", class: "btn btn-block btn", disabled: true %>
	<% end %>
<% end %>