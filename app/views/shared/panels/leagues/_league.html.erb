	<% unless Connect.where(ownerable_id: team.id, ownerable_type: team.class.to_s, connectable_type: "League", connectable_id: league.id ).present? %>
	  <%= render 'connects/team_leagues/join_league', team: team, league: league %>
	<% else %>
	  <%= render 'connects/team_leagues/leave_league', team: team, league: league %>
	<% end %>