<% if params[:chatroom_id] == chatroom.id %>
  <b><%= link_to "#{chatroom.team.proper_name} - #{chatroom.specific_id}", chatroom_messages_path(chatroom_id: chatroom.id), remote: true %></b>
<% else %>
  <span class="title"><%= link_to "#{chatroom.team.proper_name} - #{chatroom.specific_id}", chatroom_messages_path(chatroom_id: chatroom.id), remote: true %></span>
<% end %>


  
