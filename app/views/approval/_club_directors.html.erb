<table class="table table-condensed">
	<thead>
		<tr>
			<th>Club</th>
			<th>Requesting User</th>
			<th>Mobile #</th>
			<th>Date of Request</th>
			<th>Actions</th>
		</tr>
	</thead>
	<tbody>
								
							
<% club_directors.each do |cd| %>
<tr>
    <td class="v-align-middle">
    	<%= cd.roleable.name   %><br>
    </td>
    <td class="v-align-middle">
    	<span class="capitalize"><%= cd.user.full_name %></span>
    </td>
    <td class="v-align-middle">
    	<%= cd.user.mobile_phone_number   %>
    </td>
     <td class="v-align-middle">
    	<p class="small muted"><%= cd.created_at %></p>
    </td>
    <td class="v-align-middle">
    	<div class="row clearfix">
        	<div class="col-md-12">
				<%= link_to "Accept", approve_approval_path(cd.id), method: :patch, data: { confirm: 'Are you sure?', remote: true }, class: "btn btn-block btn-small btn-success row" %>
        		<%#= link_to "Delete", ad, method: :delete, data: { confirm: 'Are you sure?', remote: true }, class: "btn btn-block btn-small btn-danger" %>
        	</div>
        </div>
    </td>
</tr>
<% end %>

	</tbody>
</table>
                    