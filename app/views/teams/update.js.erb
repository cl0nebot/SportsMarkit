<% slug = "/teams/#{@object.slug}/edit" %>
history.replaceState("", "", "<%= slug %>");
$('#content').html("<%= j render 'teams/edit_form', object: @object, pictures: @pictures, videos: @videos, articles: @articles, address: @address %>")
<% if @object.valid? %>
  window.notifySuccess('Team updated');
<% else %>
  window.notifyError('Could not update');
<% end %>
