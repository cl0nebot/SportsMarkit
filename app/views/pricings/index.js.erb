$("#object-labels").empty();
$("#object-labels").append("<%= j render 'labels' %>")
<% if params[:search].present? %>
  $('#objects').empty()
  $('#objects').append("<%= j render 'pricings/list', objects: @schools.search(params[:search]) %>")
<% elsif params[:view] == "teams" %>
  $('#objects').empty()
  $('#objects').append("<%= j render 'pricings/list', objects: Team.without_schools %>")
<% elsif params[:view] == "schools" %>
  $('#objects').empty()
  $('#objects').append("<%= j render 'pricings/list', objects: @schools %>")
<% elsif params[:view] == "leagues" %>
  $('#objects').empty()
  $('#objects').append("<%= j render 'pricings/list', objects: League.all %>")
<% elsif params[:view] == "clubs" %>
  $('#objects').empty()
  $('#objects').append("<%= j render 'pricings/list', objects: Club.all %>")
<% end %>




