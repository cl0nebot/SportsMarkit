$("#object-labels").empty();
$("#object-labels").append("<%= j render 'labels' %>")
<% if params[:view] == "teams" %>
  $('#objects').empty()
  $('#objects').append("<%= j render 'pricings/list', objects: Team.without_schools %>")
<% elsif params[:view] == "schools" %>
  $('#objects').empty()
  $('#objects').append("<%= j render 'pricings/list', objects: @schools %>")
<% elsif params[:view] == "leagues" %>
  $('#objects').empty()
  $('#objects').append("<%= j render 'pricings/list', objects: League.all %>")
<% end %>
<% if params[:event] == "edit" %>
$('#<%= params[:object] %>-<%= params[:object_id] %>-tr').empty()
$('#<%= params[:object] %>-<%= params[:object_id] %>-tr').append("<%= j render 'pricings/form', object: params[:object].constantize.find(params[:object_id]) %>")
<% end %>